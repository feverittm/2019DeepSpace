{
  "java.configuration.updateBuildConfiguration": "interactive",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    ".classpath": true,
    ".project": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "java.test.config": [
    {
      "name": "includeNativesConfiguration",
      "workingDirectory": "${workspaceFolder}",
      "args": [],
      "vmargs": [],
      "env": { "PATH":"${workspaceFolder}/build/tmp/jniExtractDir"}
    }
  ],
  "java.test.defaultConfig": "includeNativesConfiguration"
}
